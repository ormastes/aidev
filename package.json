{
  "name": "aidev",
  "version": "1.0.0",
  "description": "A comprehensive, production-ready AI development platform with modular theme-based architecture, featuring 150+ implemented features across 20+ themes including enterprise security, real-time monitoring, and multi-LLM support.",
  "main": "index.js",
  "scripts": {
    "test": "jest -c config/jest/jest.config.js",
    "test:unit": "jest -c config/jest/jest.config.js --testMatch='**/tests/unit/**/*.test.ts'",
    "test:integration": "jest -c config/jest/jest.config.js --testMatch='**/tests/integration/**/*.itest.ts'",
    "test:system": "jest -c config/jest/jest.config.js --testMatch='**/tests/system/**/*.stest.ts'",
    "test:external": "jest -c config/jest/jest.config.js --testMatch='**/tests/external/**/*.etest.ts'",
    "test:env": "jest -c config/jest/jest.config.js --testMatch='**/tests/env/**/*.envtest.ts'",
    "test:coverage": "jest -c config/jest/jest.config.js --coverage",
    "cucumber": "cucumber-js",
    "build": "tsc -p config/typescript/tsconfig.json",
    "clean": "rm -rf dist",
    "check-directories": "node scripts/core/detect-directory-violations.js detect",
    "directory-report": "node scripts/core/detect-directory-violations.js report",
    "fix-directories": "node scripts/core/detect-directory-violations.js fix",
    "file-api": "node scripts/file-api.js",
    "file-api:scan": "node scripts/scan-direct-file-access.js",
    "file-api:scan:prod": "node scripts/scan-production-code.js",
    "file-api:fix": "node scripts/auto-fix-file-api.js",
    "file-api:fix:priority": "node scripts/auto-fix-file-api.js --priority --report",
    "file-api:migrate": "node scripts/migrate-all-themes.js",
    "file-api:hooks:install": "bash scripts/install-git-hooks.sh",
    "file-api:hooks:check": "bash scripts/pre-commit-file-api.sh",
    "file-api:demo": "ts-node layer/themes/infra_external-log-lib/examples/file-api-demo.ts",
    "file-api:enforce": "node scripts/scan-with-exemptions.js --fail-on-violations",
    "file-api:compliance": "node scripts/scan-production-code.js && node scripts/scan-with-exemptions.js",
    "fraud-check": "node layer/themes/infra_external-log-lib/scripts/run-fraud-check.js",
    "pre-build": "bun run file-api:enforce",
    "ci:enforce": "bun run file-api:enforce && bun run fraud-check"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@cucumber/cucumber": "^12.0.0",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/uuid": "^9.0.8",
    "bun-types": "^1.2.20",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@types/ws": "^8.18.1",
    "ajv": "^8.12.0",
    "chalk": "^5.5.0",
    "ink": "^6.1.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-table": "^3.1.0",
    "ink-text-input": "^6.0.0",
    "jsonpath-plus": "^7.2.0",
    "playwright": "^1.54.1",
    "react": "^19.1.1",
    "uuid": "^9.0.1",
    "ws": "^8.18.3"
  },
  "directories": {
    "doc": "docs"
  }
}
