{
  "name": "aidev",
  "version": "1.0.0",
  "description": "A comprehensive, production-ready AI development platform with modular theme-based architecture, featuring 150+ implemented features across 20+ themes including enterprise security, real-time monitoring, and multi-LLM support.",
  "main": "index.js",
  "scripts": {
    "test": "bun test",
    "test:all:root": "cd setup && bun run test",
    "test:bun": "bun test",
    "test:jest": "jest -c config/jest/jest.config.js",
    "test:unit": "bun test **/*.test.ts **/*.test.js",
    "test:integration": "bun test **/*.itest.ts",
    "test:system": "bun run scripts/bun-cucumber-runner.ts",
    "test:system:bun": "./scripts/run-cucumber-with-bun.sh",
    "test:system:upgrade": "./scripts/upgrade-nodejs.sh",
    "test:external": "bun test **/*.etest.ts",
    "test:env": "bun test **/*.envtest.ts",
    "test:coverage": "bun test --coverage",
    "cucumber": "cucumber-js",
    "cucumber:bun": "bun scripts/bun-cucumber-runner.ts",
    "cucumber:automated": "cucumber-js --config cucumber.yml --profile automated",
    "cucumber:manual": "cucumber-js --config cucumber.yml --profile manual",
    "cucumber:all": "cucumber-js --config cucumber.yml --profile all",
    "cucumber:dry": "cucumber-js --config cucumber.yml --profile dry-run",
    "cucumber:parallel": "cucumber-js --config cucumber.yml --profile parallel",
    "node:upgrade": "./scripts/upgrade-nodejs.sh",
    "build": "tsc -p config/typescript/tsconfig.json",
    "clean": "rm -rf dist",
    "check-directories": "node scripts/core/detect-directory-violations.js detect",
    "directory-report": "node scripts/core/detect-directory-violations.js report",
    "fix-directories": "node scripts/core/detect-directory-violations.js fix",
    "file-api": "node scripts/file-api.js",
    "file-api:scan": "node scripts/scan-direct-file-access.js",
    "file-api:scan:prod": "node scripts/scan-production-code.js",
    "file-api:fix": "node scripts/auto-fix-file-api.js",
    "file-api:fix:priority": "node scripts/auto-fix-file-api.js --priority --report",
    "file-api:migrate": "node scripts/migrate-all-themes.js",
    "file-api:hooks:install": "bash scripts/install-git-hooks.sh",
    "file-api:hooks:check": "bash scripts/pre-commit-file-api.sh",
    "file-api:demo": "ts-node layer/themes/infra_external-log-lib/examples/file-api-demo.ts",
    "file-api:enforce": "node scripts/scan-with-exemptions.js --fail-on-violations",
    "file-api:compliance": "node scripts/scan-production-code.js && node scripts/scan-with-exemptions.js",
    "fraud-check": "node layer/themes/infra_external-log-lib/scripts/run-fraud-check.js",
    "pre-build": "bun run file-api:enforce",
    "ci:enforce": "bun run file-api:enforce && bun run fraud-check",
    "test:all": "./scripts/test/run-all-tests-bun.sh",
    "test:all:virtual": "./scripts/run-all-tests-with-virtual-environment.sh",
    "test:all:safe": "./scripts/run-all-tests-with-virtual-environment.sh",
    "test:all:unit": "./scripts/run-all-tests.sh --type unit",
    "test:all:integration": "./scripts/run-all-tests.sh --type integration",
    "test:all:system": "./scripts/run-all-tests.sh --type system",
    "test:all:system:virtual": "./scripts/run-all-tests-with-virtual-environment.sh --type system",
    "test:all:coverage": "./scripts/run-all-tests.sh --coverage",
    "test:all:parallel": "./scripts/run-all-tests.sh --parallel",
    "test:all:ci": "./scripts/run-all-tests-with-virtual-environment.sh --coverage --fail-fast",
    "test:help": "./scripts/test-runner-help.sh",
    "test:e2e": "./scripts/run-playwright-tests.sh",
    "test:e2e:headed": "./scripts/run-playwright-tests.sh headed",
    "test:e2e:debug": "./scripts/run-playwright-tests.sh debug",
    "test:e2e:ui": "./scripts/run-playwright-tests.sh ui"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@cucumber/cucumber": "^12.0.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/chai": "^5.2.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/uuid": "^9.0.8",
    "bun-types": "^1.2.20",
    "chai": "^5.2.1",
    "jest": "^30.0.4",
    "puppeteer": "^24.17.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@types/ws": "^8.18.1",
    "ajv": "^8.12.0",
    "chalk": "^5.5.0",
    "ink": "^6.1.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-table": "^3.1.0",
    "ink-text-input": "^6.0.0",
    "jsonpath-plus": "^7.2.0",
    "playwright": "^1.55.0",
    "react": "^19.1.1",
    "uuid": "^9.0.1",
    "ws": "^8.18.3"
  },
  "directories": {
    "doc": "docs"
  }
}
