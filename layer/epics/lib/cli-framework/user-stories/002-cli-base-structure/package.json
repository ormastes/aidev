{
  "name": "@aidev/cli-framework",
  "version": "1.0.0",
  "description": "Type-safe CLI framework for AI Development Platform",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "aidev-cli": "./dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "cli",
    "framework",
    "typescript",
    "command-line",
    "aidev"
  ],
  "author": "AI Development Platform Team",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "inquirer": "^9.2.0",
    "ora": "^7.0.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  }
}