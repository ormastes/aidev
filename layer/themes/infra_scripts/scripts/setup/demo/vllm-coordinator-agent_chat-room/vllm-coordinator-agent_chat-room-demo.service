[Unit]
Description=AI Dev Portal Demo - vllm-coordinator-agent_chat-room
After=network.target postgresql.service

[Service]
Type=simple
User=ormastes
WorkingDirectory=/home/ormastes/dev/aidev/scripts/setup/demo/vllm-coordinator-agent_chat-room
Environment="NODE_ENV=demo"
Environment="PORT=3303"
ExecStart=/usr/bin/node server.js
Restart=on-failure
RestartSec=10

[Install]
WantedBy=multi-user.target
