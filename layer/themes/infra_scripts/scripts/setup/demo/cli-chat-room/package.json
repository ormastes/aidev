{
  "name": "cli-chat-room-demo",
  "version": "1.0.0",
  "description": "CLI Chat Room with Claude AI Agent",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "server": "ts-node src/index.ts",
    "server:enhanced": "ts-node src/server.ts",
    "client": "ts-node src/client/chat-client.ts",
    "claude": "ts-node src/agents/claude-coordinator.ts",
    "auth": "ts-node src/cli/claude-auth.ts",
    "demo": "./demo.sh",
    "all-in-one": "ts-node src/all-in-one.ts",
    "chat": "ts-node src/all-in-one.ts",
    "chat:enhanced": "ts-node src/enhanced-all-in-one.ts",
    "ollama": "ts-node src/cli/ollama-coordinator-cli.ts",
    "deepseek": "ts-node src/cli/ollama-coordinator-cli.ts demo-room DeepSeek-R1 --model deepseek-r1:32b",
    "test:ollama": "npm run build && node test/e2e-ollama-gpu.test.js",
    "test:cpu": "npm run build && node test/e2e-deepseek-cpu-usage.test.js",
    "test:e2e": "npm run build && node test/e2e-full-chat-deepseek.test.js",
    "monitor": "node scripts/monitor-cpu.js",
    "dashboard": "npm run build && node scripts/metrics-dashboard.js",
    "intercept:build": "npm run build && echo 'Interceptors ready'",
    "deepseek:intercepted": "npm run build && INTERCEPT_CONSOLE=true INTERCEPT_FILTER_HOSTS=localhost:11434 node --require ./dist/logging/preload-interceptors.js ./dist/cli/ollama-coordinator-cli.js",
    "server:intercepted": "npm run build && INTERCEPT_CONSOLE=true node --require ./dist/logging/preload-interceptors.js ./dist/index.js",
    "test:intercepted": "npm run build && INTERCEPT_CONSOLE=true INTERCEPT_LOG_DIR=./logs/test node --require ./dist/logging/preload-interceptors.js test/e2e-full-chat-deepseek.test.js",
    "test:auto-intercept": "node scripts/test-auto-intercept.js",
    "test:db-diff": "npm run build && INTERCEPT_CONSOLE=true INTERCEPT_DB_DIFF=true node --require ./dist/logging/preload-interceptors.js examples/test-database-diff.js",
    "demo:db-diff": "node examples/test-database-diff.js",
    "test:themes": "npm run build && node test/theme-agents-intercepted.test.js",
    "test:calculator": "npm run build && node test/e2e-calculator-story-reporter.test.js",
    "demo:themes": "./demo-themes-intercepted.sh",
    "start:story-reporter": "npm run build && node scripts/start-story-reporter.js",
    "start:enhanced-story-reporter": "npm run build && node scripts/start-enhanced-story-reporter.js",
    "start:fraud-checker": "npm run build && node scripts/start-fraud-checker.js",
    "start:enhanced-fraud-checker": "npm run build && node scripts/start-enhanced-fraud-checker.js",
    "start:env-monitor": "npm run build && node scripts/start-environment-monitor.js",
    "start:api-monitor": "npm run build && node scripts/start-external-api-monitor.js",
    "test": "jest",
    "test:system": "node scripts/run-system-tests.js",
    "test:system:calculator": "jest test/system/calculator",
    "server:test": "INTERCEPT_CONSOLE=true ts-node src/server.ts",
    "clean": "rm -rf dist",
    "test:system:coverage": "node scripts/run-system-tests-with-coverage.js",
    "coverage:class": "node scripts/countClassCoverage.js coverage-system",
    "analysis:In Progress": "node scripts/run-In Progress-analysis.js",
    "analysis:duplication": "node scripts/detectDuplication.js"
  },
  "keywords": [
    "chat",
    "cli",
    "websocket",
    "coordinator",
    "agent",
    "demo"
  ],
  "author": "AI Development Platform",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "diff": "^8.0.2",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.0",
    "ws": "^8.13.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/diff": "^8.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^18.18.6",
    "@types/uuid": "^9.0.3",
    "@types/ws": "^8.5.5",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "ts-morph": "^26.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "optionalDependencies": {
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0"
  }
}
