{
  "id": "deepseek-coding",
  "name": "DeepSeek R1 Coding Assistant",
  "description": "Programming help with DeepSeek R1 via Ollama",
  "server": {
    "port": 3001,
    "maxClients": 30
  },
  "coordinator": {
    "type": "ollama",
    "name": "DeepSeek R1",
    "enabled": true,
    "config": {
      "model": "deepseek-r1:32b",
      "serverUrl": "http://localhost:11434",
      "parameters": {
        "temperature": 0.1,
        "top_p": 0.95,
        "num_predict": 4096
      },
      "streaming": true,
      "contextSize": 32768
    },
    "behavior": {
      "respondToMentions": true,
      "respondToQuestions": true,
      "showTypingIndicator": true,
      "maxContextMessages": 20,
      "enabledActions": [
        "code_review",
        "explain",
        "help",
        "analyze"
      ]
    }
  },
  "client": {
    "usernamePrefix": "Dev",
    "autoReconnect": true
  },
  "rules": {
    "allowGuests": true,
    "maxMessageLength": 10000
  },
  "advanced": {
    "monitoring": {
      "enabled": true,
      "logLevel": "info"
    }
  }
}