{
  "metadata": {
    "level": "demo",
    "version": "2.1.0",
    "description": "CLI Calculator demo structure with demo and release deployment support",
    "supports_distributed_features": true,
    "supports_freeze": false
  },
  "structure": {
    "name": ".",
    "type": "directory",
    "description": "CLI Calculator demo project root",
    "children": [
      {
        "name": "src",
        "type": "directory",
        "description": "Source code for calculator application",
        "children": [
          { "name": "index.ts", "type": "file", "description": "Main application entry point" },
          { "name": "calculator.ts", "type": "file", "description": "Calculator logic implementation" },
          { "name": "cli.ts", "type": "file", "description": "Command line interface" }
        ]
      },
      {
        "name": "test",
        "type": "directory",
        "description": "Test files for calculator",
        "children": [
          { "name": "unit", "type": "directory", "description": "Unit tests" },
          { "name": "integration", "type": "directory", "description": "Integration tests" }
        ]
      },
      {
        "name": "config",
        "type": "directory",
        "description": "Configuration files",
        "children": [
          { "name": "default.json", "type": "file", "description": "Default configuration" }
        ]
      },
      {
        "name": "demo",
        "type": "directory",
        "description": "Demo artifacts and examples"
      },
      {
        "name": "release",
        "type": "directory",
        "description": "Release artifacts and deployments"
      },
      { "name": "package.json", "type": "file", "description": "NPM package configuration" },
      { "name": "README.md", "type": "file", "description": "Project documentation" },
      { "name": "tsconfig.json", "type": "file", "description": "TypeScript configuration" }
    ]
  }
}
