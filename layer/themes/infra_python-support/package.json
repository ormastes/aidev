    "@types/jest": "^29.5.5",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.8.0",
    "@types/semver": "^7.5.3",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
