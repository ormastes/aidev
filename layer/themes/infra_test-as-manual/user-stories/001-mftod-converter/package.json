{
  "name": "@aidev/test-as-manual",
  "version": "1.0.0",
  "description": "Convert MFTOD test scenarios to manual test documentation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "test-as-manual": "./bin/test-as-manual.ts"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "example": "ts-node examples/convert-tests.ts",
    "demo:bdd": "ts-node examples/bdd-demo.ts",
    "demo:enhanced": "ts-node examples/enhanced-demo.ts",
    "demo:hea": "ts-node examples/hea-demo.ts",
    "demo:values": "ts-node examples/simple-values-demo.ts",
    "demo:executive": "ts-node examples/executive-summary-demo.ts",
    "cli": "ts-node bin/test-as-manual.ts"
  },
  "keywords": [
    "test",
    "documentation",
    "mftod",
    "manual-testing",
    "converter",
    "jest",
    "mocha"
  ],
  "author": "AI Dev Team",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "marked": "^9.1.0",
    "handlebars": "^4.7.8",
    "typescript": "^5.2.2",
    "@cucumber/gherkin": "^27.0.0",
    "@cucumber/messages": "^24.0.1"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.3",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}