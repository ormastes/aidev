{
  "name": "@aidev/log-analysis-dashboard",
  "version": "1.0.0",
  "description": "Comprehensive log analysis dashboard for AI Development Platform",
  "main": "dist/src/pipe/index.js",
  "types": "dist/src/pipe/index.d.ts",
  "scripts": {
    "build": "tsc --build",
    "build:watch": "tsc --build --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:system": "jest tests/system",
    "test:system:playwright": "playwright test",
    "test:system:run": "ts-node tests/system/run-system-tests.ts",
    "test:system:embedded": "playwright test tests/system/embedded-apps/",
    "test:system:dashboard": "playwright test tests/system/embedded-apps/log-analysis-dashboard.stest.ts",
    "test:system:portal": "playwright test tests/system/embedded-apps/ai-dev-portal.stest.ts",
    "test:system:setup": "playwright test tests/system/embedded-apps/setup-configuration-ui.stest.ts",
    "test:system:monitoring": "playwright test tests/system/embedded-apps/monitoring-dashboards.stest.ts",
    "start": "node dist/src/external/http-server.js",
    "dev": "ts-node src/external/http-server.ts",
    "lint": "eslint src tests --ext .ts,.tsx",
    "lint:fix": "eslint src tests --ext .ts,.tsx --fix",
    "clean": "rm -rf dist coverage",
    "prepare": "npm run build",
    "dashboard": "npm run dev"
  },
  "keywords": [
    "log-analysis",
    "dashboard",
    "monitoring",
    "real-time",
    "typescript",
    "react",
    "websocket"
  ],
  "author": "AI Development Platform",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "chart.js": "^4.4.1",
    "react-chartjs-2": "^5.2.0",
    "date-fns": "^3.3.1",
    "lodash": "^4.17.21",
    "csv-writer": "^1.6.0",
    "jspdf": "^2.5.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/lodash": "^4.14.202",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/ws": "^8.5.10",
    "@types/jest": "^29.5.12",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "vite": "^5.1.4",
    "@vitejs/plugin-react": "^4.2.1",
    "playwright": "^1.41.2",
    "@playwright/test": "^1.41.2"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.ts",
      "**/tests/**/*.itest.ts",
      "**/tests/**/*.stest.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/*.test.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "eslintConfig": {
    "extends": [
      "@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "react",
      "react-hooks"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/explicit-function-return-type": "warn",
      "react/react-in-jsx-scope": "off"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
}