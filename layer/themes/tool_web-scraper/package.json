{
  "name": "@aidev/tool_web-scraper",
  "version": "1.0.0",
  "description": "Web Scraping Theme for AI Development Platform",
  "main": "pipe/index.ts",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "tsc",
    "lint": "eslint pipe/**/*.ts children/**/*.ts tests/**/*.ts src/**/*.ts --ext .ts",
    "start": "node dist/src/server.js",
    "cli": "node dist/src/cli.js",
    "dev": "ts-node src/server.ts"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.5.0",
    "playwright": "^1.40.0",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "xml2js": "^0.6.2",
    "jsdom": "^23.0.1",
    "mongodb": "^6.3.0",
    "pg": "^8.11.3",
    "aws-sdk": "^2.1503.0",
    "sqlite3": "^5.1.6",
    "node-cron": "^3.0.3",
    "robots-parser": "^3.0.1",
    "tough-cookie": "^4.1.3",
    "proxy-agent": "^6.3.1",
    "user-agents": "^1.1.18",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@types/xml2js": "^0.4.14",
    "@types/jsdom": "^21.1.6",
    "@types/pg": "^8.10.9",
    "@types/tough-cookie": "^4.0.5",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/tests"],
    "testMatch": ["**/*.test.ts"],
    "collectCoverageFrom": [
      "pipe/**/*.ts",
      "children/**/*.ts",
      "src/**/*.ts",
      "!**/*.test.ts",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"]
  },
  "keywords": ["web", "scraper", "crawler", "data", "extraction", "theme"],
  "author": "AI Development Platform",
  "license": "MIT"
}