{
  "name": "@aidev/vllm-coordinator",
  "version": "0.1.0",
  "description": "vLLM Coordinator Agent for AI Development Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli/vllm-coordinator-cli.js",
    "dev": "ts-node src/cli/vllm-coordinator-cli.ts",
    "setup": "ts-node scripts/setup-vllm.ts",
    "vllm:install": "node dist/cli/vllm-coordinator-cli.js install",
    "vllm:test": "node dist/cli/vllm-coordinator-cli.js test",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:system": "jest --testPathPattern=tests/system",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "vllm",
    "coordinator",
    "agent",
    "ai",
    "chat"
  ],
  "author": "AI Dev Team",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "openai": "^4.28.0",
    "socket.io-client": "^4.7.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}