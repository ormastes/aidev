{
  "name": "@aidev/portal_security",
  "version": "1.0.0",
  "description": "Centralized authentication and security services for AI Development Platform",
  "main": "dist/pipe/index.js",
  "types": "dist/pipe/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "tsc --watch",
    "clean": "rm -rf dist coverage",
    "proxy": "ts-node src/proxy-server.ts",
    "proxy:local": "NODE_ENV=dev-local ts-node src/proxy-server.ts",
    "proxy:dev": "NODE_ENV=development ts-node src/proxy-server.ts",
    "proxy:demo": "NODE_ENV=demo ts-node src/proxy-server.ts",
    "proxy:release": "NODE_ENV=release ts-node src/proxy-server.ts",
    "proxy:all": "ts-node src/multi-proxy-manager.ts",
    "proxy:status": "ts-node src/proxy-status.ts"
  },
  "keywords": [
    "authentication",
    "security",
    "sso",
    "session",
    "jwt"
  ],
  "dependencies": {
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "http-proxy-middleware": "^2.0.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}