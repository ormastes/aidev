{
  "name": "@aidev/story-reporter",
  "version": "1.0.0",
  "description": "Comprehensive Agile BDD Story Management and Reporting Tool",
  "main": "dist/index.js",
  "bin": {
    "story-reporter": "dist/cli/story-reporter-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/server.ts",
    "start": "node dist/server.js",
    "cli": "ts-node src/cli/story-reporter-cli.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist coverage stories story-reports",
    "example": "ts-node examples/story-report-example.ts"
  },
  "keywords": [
    "agile",
    "bdd",
    "story",
    "reporter",
    "test",
    "coverage",
    "quality"
  ],
  "author": "AI Development Platform",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "express": "^4.18.2",
    "inquirer": "^8.2.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/inquirer": "^8.2.6",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.18.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "src",
    "examples",
    "README.md",
    "ENHANCED_FEATURES.md"
  ]
}