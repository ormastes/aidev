{
  "description": "Examples of code navigation features",
  "examples": [
    {
      "name": "Go to definition",
      "request": {
        "method": "goToDefinition",
        "params": {
          "file": "/src/app.ts",
          "line": 15,
          "character": 20
        }
      },
      "response": [
        {
          "file": "/src/utils/helpers.ts",
          "range": {
            "start": { "line": 10, "character": 0 },
            "end": { "line": 10, "character": 15 }
          }
        }
      ]
    },
    {
      "name": "Find all references",
      "request": {
        "method": "findReferences",
        "params": {
          "file": "/src/models/User.ts",
          "line": 5,
          "character": 10
        }
      },
      "response": [
        {
          "file": "/src/models/User.ts",
          "range": {
            "start": { "line": 5, "character": 6 },
            "end": { "line": 5, "character": 10 }
          }
        },
        {
          "file": "/src/services/auth.ts",
          "range": {
            "start": { "line": 3, "character": 15 },
            "end": { "line": 3, "character": 19 }
          }
        },
        {
          "file": "/src/controllers/userController.ts",
          "range": {
            "start": { "line": 8, "character": 20 },
            "end": { "line": 8, "character": 24 }
          }
        }
      ]
    },
    {
      "name": "Get document symbols",
      "request": {
        "method": "getDocumentSymbols",
        "params": {
          "file": "/src/services/dataService.ts"
        }
      },
      "response": [
        {
          "name": "DataService",
          "kind": "class",
          "location": {
            "file": "/src/services/dataService.ts",
            "range": {
              "start": { "line": 10, "character": 0 },
              "end": { "line": 50, "character": 1 }
            }
          }
        },
        {
          "name": "constructor",
          "kind": "constructor",
          "location": {
            "file": "/src/services/dataService.ts",
            "range": {
              "start": { "line": 12, "character": 2 },
              "end": { "line": 15, "character": 3 }
            }
          },
          "containerName": "DataService"
        },
        {
          "name": "fetchData",
          "kind": "method",
          "location": {
            "file": "/src/services/dataService.ts",
            "range": {
              "start": { "line": 20, "character": 2 },
              "end": { "line": 30, "character": 3 }
            }
          },
          "containerName": "DataService"
        }
      ]
    }
  ]
}